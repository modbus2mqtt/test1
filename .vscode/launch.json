    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python-Debugger: Aktuelle Datei",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
             "args": ["create" ], 
            "console": "integratedTerminal"
        },
        {
            "name": "pulls dependency with pullrequest",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/bin/pulls.py",
             "args": ["dependencies", "pull", "--pullrequest","test3:2" ], 
            "console": "integratedTerminal"
        }
    ]
}